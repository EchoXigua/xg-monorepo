{
  "name": "@xigua/xg-monorepo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "keywords": [],
  "scripts": {
    "create": "pnpm  --filter='./packages/create-project' run create --targetDir=apps",
    "test": "pnpm -C backend/manage-back run dev",
    "dev": "pnpm -r --parallel --filter './frontend/*' --filter './backend/*' run dev",
    "dev:frondend": "pnpm -r --parallel --filter='./frontend/*' run dev",
    "dev:backend": "pnpm -r --parallel --filter='./backend/*' run dev",
    "build:all": "nx run-many --target=build --all --parallel",
    "build:affected": "nx affected --target=build --parallel",
    "clear:cache": "nx reset",
    "clear:modules": "rimraf node_modules",
    "clear:all": "pnpm -r --parallel run clear:modules && echo 'clear finish'",
    "prepare": "pnpm -r --filter='./packages/create-project' run build"
  },
  "author": "xigua",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "nx": "19.5.7",
    "@nx/eslint": "19.5.7",
    "@nx/vite": "19.5.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}